<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <display-name>mavelwebapp</display-name>
    <servlet>
    	<description>
    	</description>
    	<display-name>HelloServlet</display-name>
    	<servlet-name>HelloServlet</servlet-name>
    	<servlet-class>com.java.HelloServlet</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>SelectOwnerName</display-name>
    	<servlet-name>SelectOwnerName</servlet-name>
    	<servlet-class>com.servlet.SelectOwnerName</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>OwnerName</display-name>
    	<servlet-name>OwnerName</servlet-name>
    	<servlet-class>com.servlet.OwnerName</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>HelloServlet</servlet-name>
    	<url-pattern>/HelloServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>SelectOwnerName</servlet-name>
    	<url-pattern>/SelectOwnerName</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>OwnerName</servlet-name>
    	<url-pattern>/OwnerName</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        </welcome-file-list>
    </web-app>
